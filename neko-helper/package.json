    "@types/node": "^24.1.0",
    "@types/number-to-words": "^1.2.3",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "8.57.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@noble/ed25519": "^3.0.0",
    "@noble/hashes": "^2.0.1",
